@section Scripts {
    <script>
        document.addEventListener("DOMContentLoaded", function () {
            stickyNav(); // Calls the sticky navigation JavaScript function
            // Initialize wallet connect button or any other JavaScript initialization
        });

        function stickyNav() {
            const header = document.querySelector(".header");
            window.addEventListener("scroll", function () {
                let offset = window.scrollY;
                if (offset > 10) {
                    header.classList.add("active");
                } else {
                    header.classList.remove("active");
                }
            });
        }

        function openNavigation() {
            // Functionality for opening navigation - replace with actual Redux-like handling
            console.log("Navigation opened");
        }
    </script>
    <script>
        var hero4Slider = new Swiper('.swiper-container', {
            loop: true,
            speed: 1000,
            autoplay: {
                delay: 300,
                disableOnInteraction: false,
            },
            slidesPerView: "auto",
            spaceBetween: 50,
            direction: "horizontal",
            loopAdditionalSlides: 10,
            watchSlidesProgress: true,

            onSwiper: function (mySwiper) {
                var slidersCount = mySwiper.params.loop
                    ? mySwiper.slides.length - 2
                    : mySwiper.slides.length;
                var widthParts = 100 / slidersCount;
                var step = document.querySelector(".fn_cs_roadmap .step_in");
                roadmapStep(mySwiper, step, widthParts);
            },

            onSlideChange: function (mySwiper) {
                var slidersCount = mySwiper.params.loop
                    ? mySwiper.slides.length - 2
                    : mySwiper.slides.length;
                var widthParts = 100 / slidersCount;
                var step = document.querySelector(".fn_cs_roadmap .step_in");
                roadmapStep(mySwiper, step, widthParts);
            },
        });
        VANTA.WAVES({
            el: "#vanta-effect",
            mouseControls: true,
            touchControls: true,
            minHeight: 200.00,
            minWidth: 200.00,
            scale: 1.00,
            scaleMobile: 1.00,
            color: 0x51136d,       // Darker tone at the start of the gradient
            color2: 0x120f2f,      // Mid gradient color
            shininess: 50.00,      // Adjust shininess of the water effect
            waveHeight: 20.00,     // Adjust wave height
            waveSpeed: 0.5         // Adjust wave speed
        });
    </script>
    <script>
        document.addEventListener("DOMContentLoaded", function () {
            const collection = document.querySelector(".fn_cs_collection");
            const items = collection.querySelectorAll(".item");
            const itemsLength = items.length;

            setInterval(function () {
                let numberOne = Math.floor(Math.random() * itemsLength);
                let numberTwo;

                do {
                    numberTwo = Math.floor(Math.random() * itemsLength);
                } while (numberTwo === numberOne);

                const firstDiv = items[numberOne];
                const secondDiv = items[numberTwo];
                const firstImage = firstDiv.querySelector("input").value;
                const secondImage = secondDiv.querySelector("input").value;

                firstDiv.classList.add("ready");
                secondDiv.classList.add("ready");

                setTimeout(function () {
                    // Swap images
                    firstDiv.querySelector("input").value = secondImage;
                    firstDiv.querySelector(".abs_img").style.backgroundImage = `url(${secondImage})`;

                    secondDiv.querySelector("input").value = firstImage;
                    secondDiv.querySelector(".abs_img").style.backgroundImage = `url(${firstImage})`;

                    firstDiv.classList.remove("ready");
                    secondDiv.classList.remove("ready");
                }, 500);
            }, 2000);
        });
    </script>
}

